page']}>\n            {loading && <Loader/>}\n            {isValid && <ResetPassword token={token}/>}\n        </div>\n    );\n};\n\nexport default ResetPasswordPage;","/Users/andreyshkurenko/Projects/MyProjects/Portfolio/Andrewgram/client/src/components/AuthComponents/ResetPassword.js",[],"/Users/andreyshkurenko/Projects/MyProjects/Portfolio/Andrewgram/client/src/components/Loaders/Loader.js",[],{"ruleId":"96","replacedBy":"97"},{"ruleId":"98","replacedBy":"99"},{"ruleId":"100","severity":1,"message":"101","line":2,"column":8,"nodeType":"102","messageId":"103","endLine":2,"endColumn":12},{"ruleId":"100","severity":1,"message":"104","line":5,"column":8,"nodeType":"102","messageId":"103","endLine":5,"endColumn":13},{"ruleId":"100","severity":1,"message":"105","line":11,"column":12,"nodeType":"102","messageId":"103","endLine":11,"endColumn":19},{"ruleId":"100","severity":1,"message":"106","line":11,"column":21,"nodeType":"102","messageId":"103","endLine":11,"endColumn":31},{"ruleId":"107","severity":1,"message":"108","line":46,"column":26,"nodeType":"109","messageId":"110","endLine":46,"endColumn":27,"suggestions":"111"},{"ruleId":"107","severity":1,"message":"108","line":46,"column":51,"nodeType":"109","messageId":"110","endLine":46,"endColumn":52,"suggestions":"112"},{"ruleId":"107","severity":1,"message":"113","line":5,"column":38,"nodeType":"109","messageId":"110","endLine":5,"endColumn":39,"suggestions":"114"},{"ruleId":"107","severity":1,"message":"115","line":5,"column":46,"nodeType":"109","messageId":"110","endLine":5,"endColumn":47,"suggestions":"116"},{"ruleId":"107","severity":1,"message":"117","line":5,"column":48,"nodeType":"109","messageId":"110","endLine":5,"endColumn":49,"suggestions":"118"},{"ruleId":"107","severity":1,"message":"108","line":5,"column":50,"nodeType":"109","messageId":"110","endLine":5,"endColumn":51,"suggestions":"119"},{"ruleId":"107","severity":1,"message":"120","line":5,"column":56,"nodeType":"109","messageId":"110","endLine":5,"en